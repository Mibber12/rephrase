<%= simple_form_for(@job, html: {class: 'form-horizontal'}) do |form| %>
  <p>
  <%= form.input :title, label: "Job title" %>
  </p>
  <p>
  <%= form.input :description, label: "Job description" %>
  </p>
  <p>
  <%= form.input :company, label: "Your company" %>
  <p>
  <%= form.input :url, label: "Link to job" %>
  </p>
  <p>
  <%= form.button :submit %>
  </p>
<% end %>

